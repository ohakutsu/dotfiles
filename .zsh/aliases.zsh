# Aliases
# Base: https://github.com/ohmyzsh/ohmyzsh/blob/e9e8c6b54d594109041bdd4bc3902b40f9ae8849/plugins/common-aliases/common-aliases.plugin.zsh

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

alias l='ls -GlFh'   #size,show type,human readable
alias la='ls -GlAFh' #long list,show almost all,show type,human readable
alias lt='ls -GltFh' #long list,sorted by date,show type,human readable
alias ll='ls -Gl'    #long list
alias ldot='ls -Gld .*'
alias ls='ls -G'
